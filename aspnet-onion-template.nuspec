<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>AHutak.OnionArchitecture.AspNet</id>
    <version>2.0.0</version>
    <authors>Anatolii Hutak</authors>
    <title>ASP.NET Onion Architecture Solution template</title>
    <description> This is a solution templates package for creating an ASP.NET application following the principles of Onion Architecture.</description>
    <releaseNotes>
      Release includes the following improvements:
      - gRPC solution template added to the package,
      - added support for .NET 7,
      - all dependencies upgraded to latest versions,
      - Persistence.Migrations project is made optional
	  </releaseNotes>
    <projectUrl>https://github.com/agutak/aspnet-onion-template</projectUrl>
    <repository type="git" url="https://github.com/agutak/aspnet-onion-template.git" branch="master" commit="e1c65e4524cd70ee6e22abe33e6cb6ec73938cb3" />
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <tags>onion-architecture templates csharp dotnet dotnet-new</tags>
    <icon>icon.png</icon>
    <readme>README.md</readme>
    <packageTypes>
      <packageType name="Template" />
    </packageTypes>
    <dependencies>
      <group targetFramework="net6.0">
        <dependency id="coverlet.collector" version="3.2.0" />
        <dependency id="FluentAssertions" version="6.8.0" />
        <dependency id="Grpc.AspNetCore" version="2.50.0" />
        <dependency id="Microsoft.AspNetCore.Mvc.Testing" version="6.0.11" />
        <dependency id="Microsoft.EntityFrameworkCore.SqlServer" version="7.0.1" />
        <dependency id="Microsoft.EntityFrameworkCore.Tools" version="7.0.1" />
        <dependency id="Microsoft.Extensions.DependencyInjection.Abstractions" version="7.0.0" />
        <dependency id="Microsoft.Extensions.Options.ConfigurationExtensions" version="7.0.0" />
        <dependency id="Microsoft.NET.Test.Sdk" version="17.4.1" />
        <dependency id="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" version="1.17.0" />
        <dependency id="MongoDB.Driver" version="2.18.0" />
        <dependency id="Moq" version="4.18.3" />
        <dependency id="MSTest.TestAdapter" version="3.0.0" />
        <dependency id="MSTest.TestFramework" version="3.0.0" />
        <dependency id="Swashbuckle.AspNetCore" version="6.4.0" />
      </group>
      <group targetFramework="net7.0">
        <dependency id="coverlet.collector" version="3.2.0" />
        <dependency id="FluentAssertions" version="6.8.0" />
        <dependency id="Grpc.AspNetCore" version="2.50.0" />
        <dependency id="Microsoft.AspNetCore.Mvc.Testing" version="7.0.0" />
        <dependency id="Microsoft.EntityFrameworkCore.SqlServer" version="7.0.1" />
        <dependency id="Microsoft.EntityFrameworkCore.Tools" version="7.0.1" />
        <dependency id="Microsoft.Extensions.DependencyInjection.Abstractions" version="7.0.0" />
        <dependency id="Microsoft.Extensions.Options.ConfigurationExtensions" version="7.0.0" />
        <dependency id="Microsoft.NET.Test.Sdk" version="17.4.1" />
        <dependency id="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" version="1.17.0" />
        <dependency id="MongoDB.Driver" version="2.18.0" />
        <dependency id="Moq" version="4.18.3" />
        <dependency id="MSTest.TestAdapter" version="3.0.0" />
        <dependency id="MSTest.TestFramework" version="3.0.0" />
        <dependency id="Swashbuckle.AspNetCore" version="6.4.0" />
      </group>
    </dependencies>
    <frameworkReferences>
      <group targetFramework="net6.0">
        <frameworkReference name="Microsoft.AspNetCore.App" />
        <frameworkReference name="Microsoft.NETCore.App" />
      </group>
      <group targetFramework="net7.0">
        <frameworkReference name="Microsoft.AspNetCore.App" />
        <frameworkReference name="Microsoft.NETCore.App" />
      </group>
    </frameworkReferences>
  </metadata>  
  <files>
    <file src="templates\aspnet.webapi\.template.config\icon.png" />
    <file src="README.md" />
    <file src=".\templates\**" target="content" exclude="**\bin\**;**\obj\**;**\.vs\**;**\.vscode\**;content\Directory.Build.*;**\.git\**;**\.github\**;**\TestResults\**;" />
  </files>
</package>
